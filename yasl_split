#!./yasl_linux

{
	"Args error\n" :0
	0: print
	exit
}	arg_err

{
	"This is not a separator\n" :0
	0: print
	exit
}	no_sep

{
	"There is no string\n" :0
	0: print
	#
	exit
}	no_string

{
	0: = :0
	1: = :0
	0: == ? ("\n" :2) : (0: = :2)
	0: !
	0: # :0
}	ft_split

{
	2: print
	2: # :2
}	print_res

{
	1: ][ :1

	1 :1
	1: != ? no_sep
	0: ][ :0
	0: = :0
	0 :0
	0: == ? no_string

	@ft_split
	1 :2
	2: @print_res
	"\n" :2
	2: print
}	ft_exec

{
	!
	0: = :1
	!
	ft_exec
}	setup

3 :0
0: != ? arg_err : (0: setup)
