#!./yasl_linux

{
	"No enough args\n" :0
	0: print
	exit
}	no_enough_arg

{
	"Must be > to 0\n" :0
	0: print
	exit
}	pos

{
	"Must be integer\n" :0
	0: print
	exit
}	err_int

0: 3 < ? no_enough_arg

{
	0: = :1
	1: print
}	aff_once

{
	2: = :1
	1 :2
	2: + :2
}	copy_and_increment

{
	2: copy_and_increment
	1: #aff_once
	"\n" :1
	1: print
	! :0
	# :0
}	loop

{
	0 :0
	0: - :2
	2: = :2
	0 :2
	2: == ? err_int
}	check_int

// Skip `./yasl_aff_param`
!

0: check_int
2: = :2

# :0
0 :2
2: <= ? pos : (0: @loop)
