#!./yasl_linux

{
	"No enough args\n" :0
	0: print
	exit
}	no_enough_arg

0: 3 < ? no_enough_arg

{
	0: = :1
	1: print
}	aff_once

{
	2: = :1
	1 :2
	2: + :2
}	copy_and_increment

{
	2: copy_and_increment
	1: #aff_once
	"\n" :1
	1: print
	! :0
	# :0
}	loop

// Skip `./yasl_aff_param`
!

// Loop on parameters
0: eval :2

# :0
0: @loop

$0:@loop : 
